    {
      "name": "RSI_ANALYZER_NEW",
      "enabled": true,
      "weight": 0.15,
      "priority": 4,
      "minConfidence": 35,
      "maxConfidence": 100
    }
  ],
  "indicators": {
    "ema": {
      "fastPeriod": 9,
      "slowPeriod": 21,
      "baseConfidence": 0.5,
      "strengthMultiplier": 0.2
