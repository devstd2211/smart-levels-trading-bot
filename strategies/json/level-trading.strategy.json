  "trendAnalysis": {
    "minCandlesRequired": 20,
    "strongTrendStrength": 0.8,
    "flatTrendStrength": 0.3,
    "unclearTrendStrength": 0.0,
    "zigzagMinDepth": 5,
    "recentSwingMaxAge": 20
