    "rsi": {
      "period": 14,
      "oversold": 30,
      "overbought": 70,
      "neutralZone": {
        "min": 45,
        "max": 55
      },
      "extreme": {
        "low": 20,
        "high": 80
      }
    }
  },
  "filters": {
    "blindZone": {
      "minSignalsForLong": 2,
      "minSignalsForShort": 2,
      "longPenalty": 0.85,
      "shortPenalty": 0.90
