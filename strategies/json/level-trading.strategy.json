    {
      "name": "EMA_ANALYZER_NEW",
      "enabled": true,
      "weight": 0.30,
      "priority": 2,
      "minConfidence": 40,
      "maxConfidence": 100
    },
    {
      "name": "TREND_DETECTOR_ANALYZER_NEW",
      "enabled": true,
      "weight": 0.20,
      "priority": 3,
      "minConfidence": 45,
      "maxConfidence": 100
